{
    "name": "ziiframework/composer",
    "description": "zii-framework composer",
    "keywords": ["zii-framework", "composer"],
    "type": "composer-plugin",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/ziiframework/composer/issues",
        "source": "https://github.com/ziiframework/composer"
    },
    "authors": [
        {
            "name": "Yii Team",
            "email": "team@yiiframework.com",
            "homepage": "http://www.yiiframework.com/team",
            "role": "Founder and project lead"
        }
    ],
    "require": {
        "composer-plugin-api": "^1.0 | ^2.0"
    },
    "require-dev": {
        "composer/composer": "^1.0 | ^2.0@dev",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": { "yii\\composer\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "tests\\": "tests" }
    },
    "extra": {
        "class": "yii\\composer\\Plugin",
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
